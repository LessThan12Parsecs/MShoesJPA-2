CREATE TABLE Empleado (ID_EMPLEADO INTEGER AUTO_INCREMENT NOT NULL, DTYPE VARCHAR(31), DNI VARCHAR(255), ACTIVO TINYINT(1) default 0, APELLIDOS VARCHAR(255), NOMBRE VARCHAR(255), VERSION INTEGER, DEPARTAMENTO_EMP_ID_DEPARTAMENTO INTEGER, IMPUESTO FLOAT, SALARIO FLOAT, HORAS FLOAT, SALARIO_HORA FLOAT, PRIMARY KEY (ID_EMPLEADO))
CREATE TABLE Departamento (ID_DEPARTAMENTO INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (ID_DEPARTAMENTO))
CREATE TABLE Proveedor (ID_PROVEEDOR INTEGER AUTO_INCREMENT NOT NULL, ACTIVO TINYINT(1) default 0, NOMBRE VARCHAR(255), VERSION INTEGER, PRIMARY KEY (ID_PROVEEDOR))
CREATE TABLE Proveedor_Departamento (Departamentos_prov_ID_DEPARTAMENTO INTEGER NOT NULL, Proveedores_dep_ID_PROVEEDOR INTEGER NOT NULL, PRIMARY KEY (Departamentos_prov_ID_DEPARTAMENTO, Proveedores_dep_ID_PROVEEDOR))
ALTER TABLE Empleado ADD CONSTRAINT FK_Empleado_DEPARTAMENTO_EMP_ID_DEPARTAMENTO FOREIGN KEY (DEPARTAMENTO_EMP_ID_DEPARTAMENTO) REFERENCES Departamento (ID_DEPARTAMENTO)
ALTER TABLE Proveedor_Departamento ADD CONSTRAINT PrveedorDepartamentoDpartamentosprovIDDEPARTAMENTO FOREIGN KEY (Departamentos_prov_ID_DEPARTAMENTO) REFERENCES Departamento (ID_DEPARTAMENTO)
ALTER TABLE Proveedor_Departamento ADD CONSTRAINT Proveedor_DepartamentoProveedores_dep_ID_PROVEEDOR FOREIGN KEY (Proveedores_dep_ID_PROVEEDOR) REFERENCES Proveedor (ID_PROVEEDOR)
